{
  "manifest_version": 3,
  "name": "Odb-extention",
  "short_name": "Odb",
  "description": "Пошук українськіх компанії прямо з нової вкладки",
  "version": "0.0.2",
  "start_url": "/index.html",
  "display": "minimal-ui",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "theme_color": "#FFFFFF",
  "icons": [
    {
      "src": "favicon-16x16.png",
      "type": "image/png",
      "sizes": "16x16",
      "purpose": "any"
    },
    {
      "src": "favicon-32x32.png",
      "type": "image/png",
      "sizes": "32x32",
      "purpose": "any"
    },
    {
      "src": "favicon-96x96.png",
      "type": "image/png",
      "sizes": "96x96",
      "purpose": "any"
    },
    {
      "src": "favicon-128x128.png",
      "type": "image/png",
      "sizes": "128x128",
      "purpose": "any"
    },
    {
      "src": "mstile-144x144.png",
      "type": "image/png",
      "sizes": "144x144",
      "purpose": "any"
    },
    {
      "src": "maskable_icon.png",
      "type": "image/png",
      "sizes": "512x512",
      "purpose": "maskable"
    }
  ]
}
